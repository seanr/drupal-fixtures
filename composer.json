{
    "name": "deck36/drupal-fixtures",
    "type": "drupal-module",
    "description": "Drupal extension to have fixtues.",
    "keywords": ["drupal", "web", "fixtures"],
    "homepage": "https://github.com/DECK36/drupal-fixtures",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Mike Lohmann",
            "homepage": "http://deck36.de"
        },
        {
            "name": "Sean Robertson",
            "homepage": "http://webolutionary.com"
        },
        {
            "name": "Julien Roubieu"
        },
        {
            "name": "Tom Verhaeghe"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/finder": "~2.4",
        "symfony/dependency-injection": "~2.4",
        "symfony/yaml": "~2.4",
        "janschumann/dic": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*@stable"
    },
    "autoload": {
        "psr-0": {
            "Drupal\\Fixtures": "src/"
        }
    },
    "extra": {
      "branch-alias": {
        "dev-master": "1.0.x-dev"
      }
    }
}
